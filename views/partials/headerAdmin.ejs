                <ul>
                    
               <li> <a >¡Bienvenido <%= session.user.name %>! </a><a  href="/users/user"><img class="avatar-header" value ="34" src="/image/userAvatars/<%- session.user.avatar? session.user.avatar : "avatar.png" %>" alt="" width="50px"></a><a class="tipo" href="/users/logout">Cerrar Sesion</a></li>
            
                 <!-- <li><a class="tipo" href="/users/logout">Cerrar Sesion</a></li>  -->
                 </ul>
              <% }else{ %>
                <ul>
