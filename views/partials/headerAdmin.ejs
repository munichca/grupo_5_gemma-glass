            <div  class="sizeA c2" id="c2" onclick="color(this.id)" ></div>
            <div  class="sizeA c3" id="c3" onclick="color(this.id)"></div>
            <div  class="sizeA c4" id="c4" onclick="color(this.id)"></div>
            
         
        </div>
   
        <div class="container-1">

            <a href="/"> <img class="logo" src="/image/logo.png" alt="Logo"></a>
        </div>
        <div class="container-2">
            <p class="number">ADMINISTRADOR</p>
             <br>
            <form action="/search" method="GET">
                <div class="barra-busqueda">
                    <input class="search"  name="keywords" placeholder="Buscá un producto">
                    
                    <button type="submit" class="lupa-1" ><i class="fa fa-search "></i></button>
                </div>
            </form>
            <!-- este button estaba dentro del form por eso se cortaba
            agregue codigo en la funcion -->
            <button type="submit" class="lupa" onclick="mostrar()" id="lupa" ><i class="fa fa-search "></i></button>
        </div>
        <form class="searchForm" action="/search" method="GET">
        <div id="buscador">
                <input class="search-2" name="keywords"  placeholder="Buscá un producto" id="search_2" >
                <button type="submit" class="lupa-2"onclick="mostrar()" ><i class="fa fa-search "></i></button>
        </div>
        </form>
        <div class="container-3">
